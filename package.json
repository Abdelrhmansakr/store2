{
    "name": "store",
    "version": "1.0.0",
    "main": "index.js",
    "repository": "https://github.com/Abdelrhmansakr/store.git",
    "license": "MIT",
    "scripts": {
        "dev": "nodemon src/index.ts",
        "build": "tsc",
        "start": "npm run build && node build/index.js",
        "migration:run": "db-migrate up",
        "format": "prettier --write src/**/*.ts",
        "test": "export NODE_ENV=test && db-migrate up --env test && tsc && jasmine-ts && db-migrate down",
        "jasmine-ts": "jasmine-ts",
        "testj": "npm run build && npm run jasmine-ts",
        "jasmine": "cross-env ENV=test jasmine-ts --config jasmine.json",
        "testw": "set NODE_ENV=test && db-migrate down * --env test && db-migrate up --env test && tsc && jasmine-ts ",
        "lint": "eslint src/**/*.ts",
        "lintfix": "eslint src/**/*.ts --fix",
        "create:dev:db": "db-migrate db:create storefront_db --config database.json -e dev && npm run db:up",
        "create:test:db": "db-migrate db:create storefront_test_db --config database.json -e test",
        "drop:test:db": "db-migrate db:drop storefront_test_db",
        "db:up": "db-migrate up",
        "db:down": "db-migrate down"
    },
    "devDependencies": {
        "@types/bcrypt": "^5.0.0",
        "@types/jasmine": "^3.10.2",
        "@types/jsonwebtoken": "^8.5.6",
        "@types/morgan": "^1.9.3",
        "@types/pg": "^8.6.3",
        "@types/supertest": "^2.0.11",
        "@typescript-eslint/eslint-plugin": "^5.21.0",
        "@typescript-eslint/parser": "^5.21.0",
        "eslint": "^8.14.0",
        "eslint-config-prettier": "^8.3.0",
        "eslint-plugin-prettier": "^4.0.0",
        "jasmine": "^3.10.0",
        "jasmine-spec-reporter": "^7.0.0",
        "jasmine-ts": "^0.4.0",
        "nodemon": "^2.0.15",
        "prettier": "^2.5.1",
        "supertest": "^6.1.6",
        "ts-node": "^10.4.0",
        "typescript": "^4.5.4"
    },
    "dependencies": {
        "@types/express": "^4.17.13",
        "axios": "^0.24.0",
        "bcrypt": "^5.0.1",
        "cross-env": "^7.0.3",
        "db-migrate": "^0.11.13",
        "db-migrate-pg": "^1.2.2",
        "dotenv": "^10.0.0",
        "express": "^4.17.2",
        "express-rate-limit": "^6.0.1",
        "helmet": "^4.6.0",
        "jsonwebtoken": "^8.5.1",
        "morgan": "^1.10.0",
        "pg": "^8.7.1"
    }
}